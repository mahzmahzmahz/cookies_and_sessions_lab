<h1>Products</h1>

    <ul>
        <%cart.each do |product| %>
            <li>
                <%= product %>
            </li>
        <%end%>
    </ul>


    <%= form_tag '/products' do %><br>
		<%= label_tag "Product" %><br>
		<%= text_field_tag "product" %><br>
			
		<%= submit_tag "add to cart" %>

	<% end %>